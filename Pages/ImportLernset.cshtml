@page
@model ImportLernsetModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>Import a Learnset</h1>

<form method="post">
    Name:
    <input asp-for="Title" class="form-control mt-1"/>
    <ul id="Cards" class="p-0">
        <li class="card mt-2">
            <div class="card-body">
               <div class="form-group">
                   <label for="exampleFormControlTextarea1">Import your Learnset here:</label>
                   <textarea asp-for="TextArea" class="form-control" id="exampleFormControlTextarea1" rows="5"></textarea>
                 </div>
            </div>
        </li>
    </ul>  
    
    <input type="submit" class="btn btn-primary mt-2" value="Create Learnset"/>
</form>

