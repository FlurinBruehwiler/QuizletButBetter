@page
@model ImportStudySetModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>Import a Learnset</h1>

<form method="post">
    <table class="table mt-2">
        <thead>
         Name:
            <input asp-for="Title" class="form-control mt-1"/>
        <tr>
            <th scope="col">Import</th>
            <th scope="col">Preview</th>
        </tr>
        </thead>
        <tbody>
            <tr>
                <td class="w-[50%]">
                    <div >
                        <label for="exampleFormControlTextarea1">Import your Study Set here:</label>
                                            <textarea asp-for="TextArea" class="form-control" id="exampleFormControlTextarea1" rows="4"></textarea>
                    </div>
                    
                </td>
                <td>
                     <label for="exampleFormControlTextarea1">Preview:</label>
                    <textarea asp-for="TextArea" class="form-control" id="exampleFormControlTextarea2" rows="4"></textarea>
                </td>
            </tr>
        </tbody>
    </table>

    <input type="submit" class="btn btn-primary mt-2" value="Create Learnset"/>
    <input type="submit" class="btn btn-primary mt-2" value="Preview"/>
</form>

